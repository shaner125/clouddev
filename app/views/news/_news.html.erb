<div class="col-sm-9 single-post-card" id="article">
  <div class="card">
    <div class="card-block">
      <h4 class="post-text">
        <%= news.title %>
      </h4>
      <div class="post-content">
        <div class="dob">
          Date Published: <%= news.date %>
        </div>
        <div class="address">
          Link: <%= news.link %>
          </div>
        </div>
      </div>
  </div><!-- card -->
</div><!-- col-sm-3 -->